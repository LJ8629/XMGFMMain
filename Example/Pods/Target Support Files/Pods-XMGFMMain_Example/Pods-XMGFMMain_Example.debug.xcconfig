ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = NO
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/XMGFMBase" "$PODS_CONFIGURATION_BUILD_DIR/XMGFMMain"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/XMGFMBase/XMGFMBase.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/XMGFMMain/XMGFMMain.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "XMGFMBase" -framework "XMGFMMain"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
